    "NodeJs",
    "Terapify"
  ],
  "author": "Carlos Enrique Ram√≠rez Flores",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/linuxcarl/technical-test/issues"
  },
  "homepage": "https://github.com/linuxcarl/technical-test#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.1.1",
    "injection-js": "^2.3.1",
    "mongoose": "^5.10.9",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.15",
    "@types/mongoose": "^5.7.36",
    "@types/swagger-ui-express": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "eslint": "^7.11.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^4.3.0",
    "jest": "^26.6.0",
    "lint-staged": "^10.4.2",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.1",
    "ts-node-dev": "^1.0.0",
    "typescript": "^4.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  }
